{
    "name": "ingresse/inspetor-php",
    "type": "library",
    "description": "A SDK to communicate with the Inspetor API",
    "keywords": [
        "inspetor",
        "anti fraud",
        "protection",
        "sdk"
    ],
    "homepage": "https://inspetor.github.io/docs-backend",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Inspetor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inspetor\\": "tests/unit"
        }
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "symfony/var-dumper": "^5.0",
        "squizlabs/php_codesniffer": "*"
    }
}
